{
  "name": "chollma-eleventy",
  "version": "1.0.0",
  "description": "Personal website rebuilt using eleventy",
  "main": "index.js",
  "scripts": {
    "clean": "npx del dist",
    "serve": "npx eleventy --serve",
    "start": "npm run serve",
    "build": "ELEVENTY_ENV=dev npx eleventy",
    "build:prod": "ELEVENTY_ENV=prod npx eleventy",
    "debug": "DEBUG=* npx eleventy",
    "watch:sass": "npx sass sass:_site/css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chollma/chollma-eleventy.git"
  },
  "author": "Cooper Hollmaier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chollma/chollma-eleventy/issues"
  },
  "homepage": "https://github.com/chollma/chollma-eleventy#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.2.0",
    "moment": "^2.29.1"
  },
  "dependencies": {
    "sass": "^1.47.0"
  }
}
